@model List<School>
@{

}
@using WebTuyenSinh.Data.Entityes;
<div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
        <div class="m-3 border-bottom pb-3">
            <div class="d-flex align-items-center">
                <h4 class="card-title">Danh Sách Các Trường</h4>
                <div class="ml-auto">
                    <a href="/ManagerSchool/Save" class="btn btn-primary btn-round ml-auto"
                         >
                        <i class="ti-plus"></i>
                       Lưu
                    </a>
                </div>
            </div>
        </div>

        <div class="card-body">
          
            <div class="table-responsive pt-3">
                <table id="data-table" class="table table-bordered">
                    <thead>
                        <tr>
                            <th>
                                Mã Tỉnh
                            </th>
                            <th>
                                Tên Tỉnh
                            </th>
                            <th>
                                Mã Quận
                            </th>
                            <th>
                                Tên Quận
                            </th>
                            <th>
                                Mã Trường
                            </th>
                            <th>
                                Tên Trường
                            </th>
                            <th>
                                Khu Vực
                            </th>
                        </tr>
                    </thead>
                    <tbody id="tbbody">
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>
                                    @item.idConscious
                                </td>
                                <td>
                                    @item.NameConscious
                                </td>
                                <td>
                                    @item.idDistrict
                                </td>
                                <td>
                                    @item.NameDistrict
                                </td>
                                <td>
                                    @item.idShool
                                </td>
                                <td>
                                    @item.NameShool
                                </td>

                                <td>
                                    @item.Area
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>