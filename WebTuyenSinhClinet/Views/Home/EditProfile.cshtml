    <title>Form</title>
    <style>
        * {
            box-sizing: initial;
        }
        .img {
            width: 130px;
            height: 160px;
            overflow: hidden;
        }

        .the-img {
            display: block;
            width: 100%;
            height: 100%;
        }
        .content p {
            color: #004174 !important;
        }

        table, th, td {
            border: 1px solid black;
            border-collapse: collapse;
            text-align: center
        }

            td input {
                width: 40px;
                border: none
            }

        *,
        ::after,
        ::before {
            box-sizing: border-box;
        }

        body {
            font-family: "Times New Roman";
            font-size: 100%;
            font-weight: 400;
            line-height: 1.5;
            color: #004174;
            text-align: left;
            margin: 0;
        }

        form {
            font-size: 16px;
        }

        p {
            margin: 0;
        }

        h1,
        h2,
        h3,
        h4,
        h5,
        h6 {
            margin: 0;
        }

        body input,
        body textarea {
            outline: none;
        }

        button,
        input {
            overflow: visible;
            color: #004174;
        }

        ::-webkit-input-placeholder {
            color: #004174;
        }

        input {
            margin: 0;
            font-family: inherit;
            font-size: inherit;
            line-height: inherit;
        }

            input[type="text" i] {
                padding: 1px 2px;
                height: 20px;
                margin-top: 5px;
            }

        .d-flex {
            display: flex;
        }

        .flex-center {
            justify-content: center;
        }

        .mt-1 {
            margin-top: 5px;
        }

        .ml-3 {
            margin-left: 5px;
        }

        .fl-left {
            float: left;
        }
        /* header */
        body div.a4-size {
            width: 25cm;
            margin-top: 20px;
            margin-bottom: 20px;
            box-shadow: 5px 5px 14px 0px rgb(0 0 0 / 30%);
            padding: 30px 20px;
            margin: 0 auto;
        }

            body div.a4-size div.educate {
                position: relative;
                padding-bottom: 5px;
            }

                body div.a4-size div.educate .educate-parent {
                    font-size: 16px;
                    text-align: center;
                }

                body div.a4-size div.educate .educate-child {
                    font-weight: bold;
                }

                body div.a4-size div.educate .line-head-one {
                    border: 1px solid black;
                    position: absolute;
                    bottom: 0;
                    width: 30%;
                    left: 50%;
                    transform: translateX(-50%);
                }

                body div.a4-size div.educate .line-head-custom {
                    width: 48%;
                }

        .fw-bold {
            font-weight: bold;
        }

        /* thông tin phiếu */
        .justify-content-end {
            justify-content: flex-end !important;
        }

        body div.a4-size div.form-title {
            margin-top: 40px;
        }

        .form-title div:first-child {
            max-width: 130px;
            margin-right: 20px;
        }

        body div.a4-size div.student-avatar {
            border: 1px solid #004174;
            height: 150px;
            width: 130px;
        }

        .student-avatar-size {
            font-size: 14px;
            text-align: center;
            margin-top: 40px;
        }

        .student-avatar-note {
            margin-top: 5px;
            font-size: 14px;
            text-align: center;
        }

        body div.a4-size div.form-title > div > h6 {
            font-weight: bold;
            text-align: center;
            font-size: 18px;
            color: red;
        }

            body div.a4-size div.form-title > div > h6 > span {
                display: block;
                margin-top: 5px;
            }

        /* form info */
        body div.a4-size div.student-form-info {
            margin-left: 30px;
            margin-top: 25px;
        }

            body div.a4-size div.student-form-info p > span {
                font-weight: bold;
            }

            body
            div.a4-size
            div.student-form-info
            div.full-name-box > div.full-name-validation {
                width: 50%;
            }

        div.validation-parent {
            position: relative;
            display: inline-block;
        }

        body
        div.a4-size
        div.student-form-info
        div.full-name-box > div.validation-parent > input.full-name-property {
            border: none;
            border-bottom: 1px dotted #004174;
            width: 100%;
            font-style: italic;
            text-transform: uppercase;
        }

        /* chia col */
        .row {
            display: flex;
            flex-wrap: wrap;
            margin-left: -15px;
            margin-right: -15px;
        }

        .col {
            padding-left: 15px;
            padding-right: 15px;
            background-clip: content-box;
        }

        .c-3 {
            flex: 0 0 25%;
            max-width: 25%;
        }

        .c-4 {
            flex: 0 0 35.66667%;
            max-width: 35.66667%;
        }

        .c-5 {
            flex: 0 0 41.66667%;
            max-width: 41.66667%;
