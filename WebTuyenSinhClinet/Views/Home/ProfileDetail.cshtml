                                    <input disabled required name="sex" class="form-check-input" type="radio" value="" id="checknam">
                                    <label for="flexCheckCheckedDisabled">
                                        Nam
                                    </label>

                                </div>
                                <div class="form-check">
                                    <input disabled required name="sex" class="form-check-input" type="radio" value="" id="checknu">
