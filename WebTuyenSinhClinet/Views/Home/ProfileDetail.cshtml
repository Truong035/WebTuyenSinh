                                            <td><input disabled type="text" class="subject3" required value="@item.subject3" /></td>
                                            <td><input disabled type="text" class="subject4" required value="@item.subject4" /></td>
                                            <td><input disabled type="text" class="subject5" required value="@item.subject5" /></td>
                                            <td><input disabled type="text" class="subject6" required value="@item.subject6" /></td>
                                            <td><input disabled type="text" class="subject7" required value="@item.subject7" /></td>
                                            <td><input disabled type="text" class="subject8" required value="@item.subject8" /></td>
                                            <td><input disabled type="text" class="subject9" required value="@item.subject9" /></td>
                                            <td>
                                                <div class="form-button-action">
                                                    <input disabled type="number" required value="@item.STT" />
                                                </div>
                                            </td>

                                        </tr>
                                    } }

                            </tbody>




                        </table>

                    </div>
