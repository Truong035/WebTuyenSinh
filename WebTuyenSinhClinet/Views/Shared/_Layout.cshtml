    <script>
       const Url = "https://utc2.tk"
      ///const Url = "https://localhost:44396"
        $("document").ready(function () {
            $(".btn-gototop").hide()
            $(window).scroll(function () {
                if ($(this).scrollTop() >= 100) {
                    $(".btn-gototop").show("slow")
                }
                else {
                    $(".btn-gototop").hide("slow")
                }
            })
            $(".btn-gototop").click(function () {
                $("html,body").animate({
                    scrollTop: 0
                }, 500)
            })
        })
        $('#particles1').hide();
        AOS.init();
    </script>
