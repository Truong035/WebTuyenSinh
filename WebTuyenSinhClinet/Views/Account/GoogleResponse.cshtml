@model RegisterRequest
@using WebTuyenSinh_Application.Modell

<h1 class="bg-info text-white">Login</h1>
<div class="text-danger" asp-validation-summary="All"></div>

<form asp-action="Register" method="post">

    <div class="form-group">
        <label asp-for="Email"></label>
        <input asp-for="Email" disabled class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="UseName"></label>
        <input asp-for="UseName" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Telephone"></label>
        <input asp-for="Telephone" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Password"></label>
        <input asp-for="Password" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="ConfirmPassword"></label>
        <input asp-for="ConfirmPassword" class="form-control" />
    </div>
    <div class="form-group">
        <label asp-for="Adress"></label>
        <input asp-for="Adress" class="form-control" />
    </div>
</form>

<div style="background:#00ff90">
    <h2>Important Links</h2>
    <p><a href="/Admin" target="_blank">Admin</a></p>
    <p><a href="/Role" target="_blank">Role</a></p>
    <p><a href="/Claims" target="_blank">Claims</a></p>
</div>