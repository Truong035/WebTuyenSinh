@model RegisterRequest
@using WebTuyenSinh_Application.Modell

@section Scripts{
    <script>
        $(document).ready(function () {
            $("#resgisModal").modal("show");
        })

    </script>
}
<div class="modal fade"
     id="resgisModal"
     tabindex="-1"
     aria-labelledby="exampleModalLabel"
     aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content">
            <div class="modal-header" style="background-color: #FFFDE7 ;">

                <img class="modal-title" id="exampleModalLabel" style="width: 30%; display: block; margin-left: 35%;" src="https://utc2.edu.vn/upload/company/favicon-15725981454.ico" />

            </div>
            <div class="modal-body" style="background-color:#FAFAFA;">
                <div class="text-danger" asp-validation-summary="All"></div>

                <form asp-action="Register" method="post">

                    <div class="form-group">
                        <label asp-for="Email"></label>
                        <input asp-for="Email" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="UseName"></label>
                        <input asp-for="UseName" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="Telephone"></label>
                        <input asp-for="Telephone" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="Password"></label>
                        <input asp-for="Password" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="ConfirmPassword"></label>
                        <input asp-for="ConfirmPassword" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="Adress"></label>
                        <input asp-for="Adress" class="form-control" />
                    </div>
                    <div class="form-group">
                        <label asp-for="BrithDay"></label>
                        <input asp-for="BrithDay" type="date" class="form-control" />
                    </div>
                  
                    <button type="submit" class="btn btn-success btn-block mb-4 mt-2"> Đăng Ký</button>
                </form>

            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-mdb-dismiss="modal">
                    Đóng
                </button>
            </div>
        </div>
    </div>
</div>
<div class="p-5 text-center bg-light jumbotron" style="margin-top: 50px;" data-aos="flip-left"
     data-aos-easing="ease-out-cubic"
     data-aos-duration="2000">
    <h1>Trường đại học giao thông vận tải phân hiệu tại thành phố Hồ Chí Minh</h1>
